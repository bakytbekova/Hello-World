#include <stdio.h>

int main ()
{ 
  int a,b:
  
  printf("\n\tEnter a: ");
  scanf("%d", &a);
  printf("\n\tEnter b: ");
  scanf("%d", &b);
  
  printf("\n\ta + b*b = %d", a*a + b*b); 
  printf("\n\ta - b*b = %d", a*a - b*b);
  printf("\n\ta * b*b = %d", a*a * b*b);
  printf("\n\ta : b*b = %d", a*a / b*b);
  
  printf("\n");
  return 0
  }
