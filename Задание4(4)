#include <stdio>

int main ()
{
  int a, b;
  
  printf("\n\tEnter a: ");
  scanf("%d", &a);
  printf("\n\tEnter b: ");
  scanf("%d", &b);
  
  printf("\n\ta + b = %d", a+b);
  printf("\n\ta - b = %d", a-b);
  printf("\n\ta * b = %d", a*b);
  printf("\n\ta : b = %d", a/b);
 
  printf("\n");
  return 0
  }
  
